package Replit;

public class HomeWork68 {

	public static void main(String[] args) {
		
		for (int i = 1; i < 8; i++) {
		    System.out.print(i);
		    for (int j = 1; j < i + 6; ++j) {
		      System.out.println(j);} 
		    for (int j = 1; j < i + 1; ++j) {
			       System.out.println(j);} 

}

}
	}

