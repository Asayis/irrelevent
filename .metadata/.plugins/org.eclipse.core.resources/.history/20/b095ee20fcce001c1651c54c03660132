package com.syntax.class04;

public class MoreNestedIf {

	public static void main(String[] args) {
		boolean didRepl = true;
		
		int assignment=7;

		if (didRepl) {
			System.out.println("How many assignements have you done");
			
			if (assignment>6) System.out.println("good marks!");
			
			else if (assignment<6 && assignment>12) System.out.println("great marks");
			
			else if (assignment>6) System.out.println("you failed");
			
			else System.out.println("top of your class mate");

		} 
		
			
		
			}
		}