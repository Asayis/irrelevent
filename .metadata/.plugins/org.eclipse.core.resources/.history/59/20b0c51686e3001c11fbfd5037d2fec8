package Playaround;

public class loopsPlayaround {

	public static void main(String[] args) {

		for (int i = 0; i < 100; i++) {
		
			
			for (int j = 0; j < 4; j++)
			System.out.println(i + " 2 " + j);
			System.out.println();
			
			{
				for (int k = 0; k < 5; k++)
					;
				System.out.println(" 3rd loop");
			}

		}

	}

}
