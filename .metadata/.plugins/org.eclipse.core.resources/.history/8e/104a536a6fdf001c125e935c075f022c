package com.syntax.class10;

public class ThreeDArrays {

	public static void main(String[] args) {
		// test is a 3d array
		String[][][] test = {
		        {
		          {"Binghamton", "Dunkirk", "Kingston" }, 
		          {"AlturasCounty seat", "Atwater", "Belmont"}
		        }, 
		        { 
		          {-4, -5, 6, 9}, 
		          {1}, 
		          {2, 3}
		        } 
		};
System.out.println(test.length);
	}

}
