package com.syntax.class10;

public class ThreeDArrays {

	public static void main(String[] args) {
		// test is a 3d array
		int[][][] test = {
		        {
		          {1, -2, 3}, 
		          {2, 3, 4}
		        }, 
		        { 
		          {-4, -5, 6, 9}, 
		          {1}, 
		          {2, 3}
		        } 
		};
System.out.println(test[test.length]);
	}

}
