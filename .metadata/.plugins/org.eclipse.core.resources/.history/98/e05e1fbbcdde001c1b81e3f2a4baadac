package com.syntax.class09;

public class HomeWork3StoreDouble {

	public static void main(String[] args) {
		
		double[] a = {2.99,4.99,5.99, 63.99};
		double[] b = new double[5];
		
		for (double a1:a) {
			System.out.print(a1+", (A) ");}
		
		System.out.println("-------------");
		
			b[0]= 112.99;
			b[1]= 114.99;
			b[2]= 116.99;
			b[3]= 118.99;
			b[4]= 1110.99;
		
			for (double b1; b1<b; b1++) {
				System.out.print(b1+", (B) ");}
			
	
		
		}
	}


